{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "dist",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "lib": [
      "esnext"
    ],
    "strict": false,
    "noUnusedLocals": true,
    "pretty": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": false,
    "noImplicitReturns": false
  }
}